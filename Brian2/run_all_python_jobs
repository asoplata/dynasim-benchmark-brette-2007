sleep 3.00
$CODEDIR/qpyjobs_memlimit_loop brian2_benchmark_COBAHH_clocksyn_lodens_500
sleep 3.00
$CODEDIR/qpyjobs_memlimit_loop brian2_benchmark_COBAHH_clocksyn_lodens_1000
sleep 3.00
$CODEDIR/qpyjobs_memlimit_loop brian2_benchmark_COBAHH_clocksyn_lodens_2000
sleep 3.00
$CODEDIR/qpyjobs_memlimit_loop brian2_benchmark_COBAHH_clocksyn_lodens_4000
sleep 3.00
$CODEDIR/qpyjobs_memlimit_loop brian2_benchmark_COBAHH_clocksyn_lodens_8000
sleep 3.00
$CODEDIR/qpyjobs_memlimit_loop brian2_benchmark_COBAHH_clocksyn_lodens_16000
sleep 3.00
$CODEDIR/qpyjobs_memlimit_loop brian2_benchmark_COBAHH_clocksyn_lodens_32000
sleep 3.00

#################### COBAHH clocksyn hidens #######################
$CODEDIR/qpyjobs_memlimit_loop brian2_benchmark_COBAHH_clocksyn_hidens_250
sleep 3.00
$CODEDIR/qpyjobs_memlimit_loop brian2_benchmark_COBAHH_clocksyn_hidens_500
sleep 3.00
$CODEDIR/qpyjobs_memlimit_loop brian2_benchmark_COBAHH_clocksyn_hidens_1000
sleep 3.00
$CODEDIR/qpyjobs_memlimit_loop brian2_benchmark_COBAHH_clocksyn_hidens_2000
sleep 3.00
$CODEDIR/qpyjobs_memlimit_loop brian2_benchmark_COBAHH_clocksyn_hidens_4000
sleep 3.00
$CODEDIR/qpyjobs_memlimit_loop brian2_benchmark_COBAHH_clocksyn_hidens_8000
sleep 3.00
$CODEDIR/qpyjobs_memlimit_loop brian2_benchmark_COBAHH_clocksyn_hidens_16000
sleep 3.00
$CODEDIR/qpyjobs_memlimit_loop brian2_benchmark_COBAHH_clocksyn_hidens_32000
