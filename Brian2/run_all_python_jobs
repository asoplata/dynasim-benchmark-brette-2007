sleep 3.00
$CODEDIR/qpyjobs_memlimit_loop brian2_benchmark_COBAHH_nosyn_8000
sleep 3.00
$CODEDIR/qpyjobs_memlimit_loop brian2_benchmark_COBAHH_nosyn_16000
sleep 3.00
$CODEDIR/qpyjobs_memlimit_loop brian2_benchmark_COBAHH_nosyn_32000
sleep 3.00


#################### COBAHH clocksyn lodens #######################
